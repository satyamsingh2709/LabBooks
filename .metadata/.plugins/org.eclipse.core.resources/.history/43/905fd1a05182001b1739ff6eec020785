package com.cg.lb4.ui;
public class Person {
	String name;
	float age;

	public Person(String name, float age) {
		super();
		this.name = name;
		this.age = age;
	}
	
	//getter and setter methods

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public float getAge() {
		return age;
	}

	public void setAge(float age) {
		this.age = age;
	}

}
